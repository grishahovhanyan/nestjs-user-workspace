{
  "name": "nestjs-user-workspace",
  "version": "1.1.0",
  "description": "Nestjs UserWorkspace",
  "author": "Grisha Hovhanyan",
  "module": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/src/main",
    "typeorm": "npx typeorm-ts-node-commonjs -d ./libs/database/src/mysql/data-source.ts",
    "migrations:create": "npx typeorm-ts-node-commonjs migration:create ./libs/database/src/mysql/migrations/$npm_config_name",
    "migrations:generate": "npm run typeorm -- migration:generate ./libs/database/src/mysql/migrations/$npm_config_name",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:rollback": "npm run typeorm -- migration:revert",
    "docker:build": "docker compose --env-file .env.dev -f docker-compose.dev.yml build --no-cache",
    "docker:up": "docker compose --env-file .env.dev -f docker-compose.dev.yml up",
    "docker:build-up": "docker compose --env-file .env.dev -f docker-compose.dev.yml up --build",
    "docker:build:api": "docker compose --env-file .env.dev -f docker-compose.dev.yml build --no-cache transport-api",
    "docker:up:api": "docker compose --env-file .env.dev -f docker-compose.dev.yml up --no-deps transport-api",
    "docker:build-up:api": "npm run docker:build:api && npm run docker:up:api"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.11.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typeorm-transactional": "^0.5.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.11.17",
    "@types/passport-jwt": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.4"
  }
}
